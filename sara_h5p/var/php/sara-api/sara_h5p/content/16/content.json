                                    {
                                        "library": "H5P.MultiChoice 1.16",
                                        "params": {
                                            "media": [],
                                            "question": "Dans un tirage au sort, si une urne contient 3 boules rouges, 2 boules bleues et 5 boules vertes, quelle est la probabilit\u00e9 de tirer une boule rouge ?",
                                            "answers": [
                                                {
                                                    "text": "1\/5",
                                                    "correct": false,
                                                    "tipsAndFeedback": {
                                                        "tip": "",
                                                        "chosenFeedback": "Essayez de recalculer la probabilit\u00e9 en tenant compte du nombre total de boules.",
                                                        "notChosenFeedback": ""
                                                    }
                                                },
                                                {
                                                    "text": "3\/10",
                                                    "correct": true,
                                                    "tipsAndFeedback": {
                                                        "tip": "",
                                                        "chosenFeedback": "Correct ! La probabilit\u00e9 est calcul\u00e9e en divisant le nombre de boules rouges par le total.",
                                                        "notChosenFeedback": ""
                                                    }
                                                },
                                                {
                                                    "text": "1\/3",
                                                    "correct": false,
                                                    "tipsAndFeedback": {
                                                        "tip": "",
                                                        "chosenFeedback": "Revoyez le calcul total des boules pour trouver la bonne fraction.",
                                                        "notChosenFeedback": ""
                                                    }
                                                },
                                                {
                                                    "text": "2\/5",
                                                    "correct": false,
                                                    "tipsAndFeedback": {
                                                        "tip": "",
                                                        "chosenFeedback": "Pensez \u00e0 la somme des boules pour d\u00e9terminer la bonne r\u00e9ponse.",
                                                        "notChosenFeedback": ""
                                                    }
                                                }
                                            ]
                                        }
                                    },
